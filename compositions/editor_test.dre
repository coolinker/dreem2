            <view y='0' x='0' bgcolor='#cccccc' height='50' width='70'>
              <text y='${((this.parent.innerheight - this.boundsheight) / 2) + this.boundsydiff}' x='${((this.parent.innerwidth - this.boundswidth) / 2) + this.boundsxdiff}' multiline='true' text='text' width='auto' height='auto'/>
            </view>
            <view x='153.5' bgcolor='#000000' y='${this.boundsydiff}' height='${this.parent.innerheight * 1}' width='1'>
              <animator/>
