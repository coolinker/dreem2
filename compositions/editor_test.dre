<composition>
  <screens>
    <screen type='browser' name='default' title='Editor Test File'>
      <view width='100%' height='100%' bgcolor='#666666'>
        <view y='31' x='30' bgcolor='rgba(165,172,169,1.00)' height='255' width='409'>
