            </animgroup>
          </view>
          <bitmap bgcolor='rgba(255,0,226,0.50)' width='50' height='50' x='${this.parent.innerwidth - this.boundswidth + this.boundsxdiff}' y='${this.parent.innerheight - this.boundsheight + this.boundsydiff}'>
